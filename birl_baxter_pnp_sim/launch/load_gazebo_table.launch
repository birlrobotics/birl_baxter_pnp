<?xml version="1.0" encoding="utf-8"?>
<launch>
<arg name="model" default="$(find birl_baxter_pnp_sim)/model/tables.xacro"/>
 <param name="robot_description" command="$(find xacro)/xacro --inorder $(arg model) gazebo:=true" />

 <node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen"
 args="-param robot_description -urdf -z 0.93 -model tables" />

</launch>
